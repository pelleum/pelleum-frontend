{
  "expo": {
      "name": "Pelleum PRE",
      "slug": "pelleum-frontend-preview",
      "version": "1.0.2",
      "orientation": "portrait",
      "icon": "./assets/preview-icon.png",
      "splash": {
          "image": "./assets/splash.png",
          "resizeMode": "cover",
          "backgroundColor": "#000000"
      },
      "updates": {
          "enabled": true,
          "fallbackToCacheTimeout": 10000
      },
      "assetBundlePatterns": [
          "**/*"
      ],
      "ios": {
          "supportsTablet": true,
          "bundleIdentifier": "com.pelleum.preview",
          "buildNumber": "4",
          "appStoreUrl": "https://apps.apple.com/us/app/pelleum/id1614359541"
      },
      "android": {
          "adaptiveIcon": {
              "foregroundImage": "./assets/preview-adaptive-icon.png",
              "backgroundColor": "#000000"
          },
          "softwareKeyboardLayoutMode": "pan",
          "permissions": [
              "ACCESS_NETWORK_STATE"
          ],
          "package": "com.pelleum.preview",
          "versionCode": 4,
          "playStoreUrl": "https://play.google.com/store/apps/details?id=com.pelleum.mobile"
      },
      "web": {
          "favicon": "./assets/favicon.png"
      },
      "userInterfaceStyle": "automatic",
      "androidStatusBar": {
          "barStyle": "light-content"
      }
  }
}