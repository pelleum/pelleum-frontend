{
  "expo": {
    "name": "Pelleum",
    "slug": "pelleum-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/dev-icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pelleum.dev",
      "buildNumber": "2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/dev-adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "softwareKeyboardLayoutMode": "pan",
      "permissions": [
        "ACCESS_NETWORK_STATE"
      ],
      "package": "com.pelleum.dev",
      "versionCode": 2,
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.pelleum.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "userInterfaceStyle": "automatic",
    "androidStatusBar": {
      "barStyle": "light-content"
    }
  }
}
