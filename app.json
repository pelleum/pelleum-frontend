{
  "expo": {
    "name": "Pelleum",
    "slug": "pelleum-frontend",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/prod-icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 10000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pelleum.mobile",
      "buildNumber": "3",
      "appStoreUrl": "https://apps.apple.com/us/app/pelleum/id1614359541"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/prod-adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "softwareKeyboardLayoutMode": "pan",
      "permissions": [
        "ACCESS_NETWORK_STATE"
      ],
      "package": "com.pelleum.mobile",
      "versionCode": 3,
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.pelleum.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "userInterfaceStyle": "automatic",
    "androidStatusBar": {
      "barStyle": "light-content"
    }
  }
}
